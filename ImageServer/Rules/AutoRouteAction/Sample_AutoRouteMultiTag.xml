<!-- 
This rule instructs the server to auto-route MR and CT studies to Macro 
-->
<rule>
	<condition expressionLanguage="dicom">
		<or>
			<equal test="$Modality" refValue="MR" />
			<equal test="$Modality" refValue="CT" />
		</or>
	</condition>
	<action>
		<auto-route
		  device="Macro" />
	</action>
</rule>